package Esempi;

public class Operazioni {

	private int x;
	private int y;

	public Operazioni(int x, int y) {
		this.x = x;
		this.y = y;
	}

	private void getSum(sumIntFun sif) {
		if (sif.sum(x, y) > 0) {
			System.out.println("Somma di " + x + " e " + y + "è > 100");
		}else {
			System.out.println("Somma di " + x + " e " + y + "è > 100");
		}
	}
}
