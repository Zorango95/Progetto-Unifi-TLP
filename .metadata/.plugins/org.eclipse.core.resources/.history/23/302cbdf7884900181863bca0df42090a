package Esempi;

public class OperazioniLambda {

	private int x;
	private int y;

	public OperazioniLambda(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public void setX(int x) {
		this.x = x;
	}

	public void setY(int y) {
		this.y = y;
	}

	public int getIdentity(IntFunId ifid) {
		return (ifid.identity(x));
	}

	public boolean getProduct(IntFunProd ifprod) {
		if (ifprod.product(x, y) > 100) {
			return true;
		} else {
			return false;
		}
	}
}
