package Esempi;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class OperazioniTest {

	@Test
	void test() {
		Operazioni op1 = new Operazioni(10,50);
		
		
		assertEquals(10, op1.getNum(x -> {
			return x;
		}));

		IntFunProd idxid = ((x,y) -> {
			return x * y;
		});
		assertEquals(100, op1.getNum(idxid));
	}

}
